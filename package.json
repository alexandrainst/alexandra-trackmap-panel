{
  "name": "alexandra-trackmap-panel",
  "version": "2.5.2",
  "description": "A map plugin to visualise GPS data as markers, hexbin, ant path, or heatmap",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint:fix": "^npm run lint --fix",
    "e2e": "^npm exec cypress install && ^npm exec grafana-e2e run",
    "e2e:update": "^npm exec cypress install && ^npm exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build"
  },
  "author": "Alexandra Institute",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/css": "^11.10.5",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "1.3.75",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/d3": "^7.4.0",
    "@types/d3-hexbin": "^0.2.2",
    "@types/jest": "^29.5.0",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet.heat": "^0.2.1",
    "@types/node": "^18.15.11",
    "@types/styled-components": "^5.1.16",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "@grafana/e2e": "10.1.1",
    "@grafana/e2e-selectors": "10.1.1",
    "@types/lodash": "^4.14.194"
  },
  "engines": {
    "node": ">=12 <21"
  },
  "dependencies": {
    "@asymmetrik/leaflet-d3": "github:asymmetrik/leaflet-d3",
    "@emotion/css": "^11.1.3",
    "@grafana/data": "^10.1.1",
    "@grafana/runtime": "^10.1.1",
    "@grafana/ui": "^10.1.1",
    "d3": "^7.8.5",
    "d3-hexbin": "^0.2.2",
    "html-react-parser": "^4.2.0",
    "leaflet": "^1.9.4",
    "leaflet-ant-path": "^1.3.0",
    "leaflet.heat": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.0",
    "styled-components": "^6.0.7",
    "tslib": "2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandrainst/alexandra-trackmap-panel"
  },
  "packageManager": "^npm@9.8.0"
}
